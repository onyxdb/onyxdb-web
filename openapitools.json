{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.11.0",
    "generators": {
      "v1.0": {
        "generatorName": "typescript-axios",
        "output": "./src/generated/api",
        "skipValidateSpec": false,
        "inputSpec": "http://localhost:9003/internal/api-docs",
        "additionalProperties": {
          "supportsES6": true,
          "useSingleRequestParameter": true
        }
      },
      "mdb": {
        "generatorName": "typescript-axios",
        "output": "./src/generated/api-mdb",
        "skipValidateSpec": false,
        "inputSpec": "http://localhost:9001/internal/api-docs",
        "additionalProperties": {
          "supportsES6": true,
          "useSingleRequestParameter": true
        }
      }
    }
  }
}