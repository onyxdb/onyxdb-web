{
  "name": "gravity-ui-example-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:styles": "stylelint '{styles,src}/**/*.scss'",
    "lint:prettier": "prettier --check '**/*.md'",
    "lint": "npm run lint:js && npm run lint:styles && npm run lint:prettier"
  },
  "dependencies": {
    "@bem-react/classname": "^1.6.0",
    "@gravity-ui/components": "^4.0.1",
    "@gravity-ui/icons": "2.13",
    "@gravity-ui/navigation": "3.3",
    "@gravity-ui/uikit": "7.9",
    "axios": "1.8.4",
    "bem-cn-lite": "4.1.0",
    "date-fns": "^4.1.0",
    "formik": "2.4.6",
    "next": "15.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@gravity-ui/browserslist-config": "^4.3.0",
    "@gravity-ui/eslint-config": "gravity-ui/eslint-config",
    "@gravity-ui/prettier-config": "^1.1.0",
    "@gravity-ui/stylelint-config": "^4.0.1",
    "@gravity-ui/tsconfig": "^1.0.0",
    "@openapitools/openapi-generator-cli": "2.18",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20.17.9",
    "@types/react": "^18.3.14",
    "@types/react-dom": "^18.3.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.0.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "postcss": "8.4.38",
    "prettier": "^3.4.2",
    "sass": "^1.82.0",
    "sass-loader": "^16.0.4",
    "stylelint": "^15.11.0",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18"
  },
  "browserslist": [
    "extends @gravity-ui/browserslist-config"
  ],
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  },
  "lint-staged": {
    "*.{scss}": [
      "stylelint --fix --quiet"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
